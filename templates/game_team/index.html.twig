{% extends 'base_admin.html.twig' %}

{% block title %}GameTeam index{% endblock %}

{% block body %}
    <h1>GameTeam index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for game_team in game_teams %}
            <tr>
                <td>{{ game_team.id }}</td>
                <td>
                    <a href="{{ path('game_team_show', {'id': game_team.id}) }}">show</a>
                    <a href="{{ path('game_team_edit', {'id': game_team.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('game_team_new') }}">Create new</a>
{% endblock %}
