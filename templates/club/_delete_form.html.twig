<form method="post" action="{{ path('club_delete', {'id': club.id , 'league': league}) }}" onsubmit="return confirm('{{ 'global.actions.deleteMessage'|trans }}');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ club.id) }}">
    <button class="btn">{{ 'global.delete' |trans }}</button>
</form>
